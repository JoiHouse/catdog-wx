.myPost {
	background-color: #f5f7fb;
	height: 100vh;
	width: 100vw;
}
.myPost-header {
	padding: 10px 26px;
}
.myPost-header .myPost-header-layout {
	padding: 10px;
	border-radius: 18px;
	background: #fff;
	display: flex;
	align-items: center;
}
.btn-round {
	height: 2em;
	width: 2em;
	border-radius: 50%;
	background-color: #3670f5;
	background-position: center;
	background-repeat: no-repeat;
}


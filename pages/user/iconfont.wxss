@font-face {
	font-family: "iconfont";
	src: url("data:application/x-font-woff2;charset=utf-8;base64,")
		format("woff2");
	font-weight: normal;
	font-style: normal;
	font-display: swap;
}

[class^="icon-"],
[class*=" icon-"] {
	font-size: 1.2em;
	color: #333;
	font-family: "iconfont" !important;
	vertical-align: -0.15em;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: inherit;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-share:before {
	content: "\e612";
}

.icon-heart:before {
	content: "\e75c";
}

.icon-heart_outlined:before {
	content: "\e75d";
}

.icon-like:before {
	content: "\e60d";
}

.icon-send:before {
	content: "\e6a3";
}

.icon-about:before {
	content: "\e75b";
	font-size: 1.39em;
}

.icon-msg:before {
	content: "\e76b";
	font-size: 1.3em;
}
.icon-comment:before {
	content: "\e768";
}

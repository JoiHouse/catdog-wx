.loginText {
  display: flex;
  flex-direction: column;
  gap: 8px;
}
.loginDialog-text {
  padding-bottom: var(--lo-dialog-padding-a);
}
.loginDialog-text > view {
  font-style: normal;
  font-weight: 700;
  font-size: 17px;
  line-height: 24px;
  color: #2b2b2f;
}
.loginDialog-text > text {
  font-style: normal;
  font-weight: 600;
  font-size: 13px;
  line-height: 18px;
  color: #5f5d6b;
}
.loginDialog-btn {
  padding: 10px 20px;
  margin: 10px;
  width: 80%;
  margin: auto;
  background-color: #ffcf22e8;
  text-align: center;
  line-height: 1.2;
  border-radius: var(--lo-dialog-radius);
  /* font-weight: 600; */
  font-size: 15px;
  line-height: 18px;
  color: #fff;
}
.loginDialog-btn-mail {
  text-align: center;
  padding: 10px 0;
  font-size: 13px;
  line-height: 18px;
  color: #5f5d6b;
}
.loginDialog-privacy{
    font-size: 12px;
    line-height: 18px;
    color: #5f5d6b;
}
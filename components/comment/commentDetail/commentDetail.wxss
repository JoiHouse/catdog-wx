.commentDetail-wrapper {
	height: 100vh;
	width: 100vw;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 999;
	background: rgba(0, 0, 0, 0.3);
}
.commentDetail {
	height: 100%;
	width: 100%;
	position: relative;
}

.commentDetail .commentDetail-content {
	width: 100%;
	height: 80%;
	overflow-y: auto;
	position: absolute;
	background: #fff;
	bottom: 0;
	border-radius: 18px 18px 0 0;
}
.commentDetail .commentDetail-content > view {
	position: relative;
	padding: 20px;
	display: flex;
	flex-direction: column;
}

.commentDetail-header {
	position: fixed;
	background: transparent;
	right: 18px;
	top: calc(20vh + 18px);
}
.commentDetail-body {
	padding-bottom: 5em;
}
.divLine {
	background: #e2e2e2e4;
	width: 96%;
	margin: 10px auto;
	height: 1px;
}
.comment-list {
	margin-top: 20px;
}
.commentDetail-closeBtn:hover {
	cursor: pointer;
}
.detail-comment-item {
	margin-bottom: 20px;
}
.detail-comment-item-t {
	padding: 6px;
	display: flex;
	align-items: center;
}
.detail-comment-item-t > image {
	height: 40px;
	width: 40px;
	border-radius: 50%;
	margin: 0 10px;
}
.detail-comment-t-user > view {
	font-size: 1em;
	font-weight: 600;
}
.detail-comment-t-user > text {
	font-size: 0.8em;
	color: #676767d9;
}
.detail-comment-item-b {
	padding: 0 20px;
}
.detail-comment-b-text {
	width: 90%;
	word-break: break-all;
	padding-bottom: 8px;
}
.detail-comment-b-pic {
	display: flex;
	gap: 6px;
}

.detail-comment-b-pic view {
	margin-top: 10px;
	width: 33%;
	padding-top: 30%;
	height: 0;
	position: relative;
}
.detail-comment-b-pic image {
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	z-index: 1;
	object-fit: cover;
	border-radius: 8px;
	box-shadow: 0px 0.5px 0.5px #efefef, 0px 1px 0.5px rgba(239, 239, 239, 0.5);
}
.comment-item-fun {
	padding: 5px 0 5px 20px;
	color: #666565;
	display: flex;
}
.comment-item-fun > view {
	margin: 5px 26px 0 auto;
	display: flex;
	align-items: center;
	justify-content: center;
	gap: 8px;
}
.detail-btns .iconfont,
.comment-item-fun .iconfont {
	color: #8c8c8c;
	display: flex;
	align-items: center;
	gap: 2px;
}

.iconfont text {
	font-size: 0.76em;
}

@font-face {
	font-family: "iconfont2"; /* Project id 4332571 */
	src: url("data:font/woff2;charset=utf-8;base64,")
			format("woff2"),
		url("data:font/woff;charset=utf-8;base64,")
			format("woff");
}
.iconfont,
[class^="icon-"],
[class*=" icon-"] {
	font-size: 1.2em;
	color: #333;
	font-family: "iconfont2" !important;
	vertical-align: -0.15em;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: inherit;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-close:before {
	content: "\e75a";
}

.icon-heart:before {
	content: "\e75c";
}

.icon-heart_outlined:before {
	content: "\e75d";
}

.icon-comment:before {
	content: "\e768";
}

.icon-square:before {
	content: "\e784";
}

.comment-2 {
	width: 90%;
	margin: auto;
	margin-top: 10px;
}
.comment-2-user {
	width: 100%;
	height: 40px;
	background-color: #fff;
	border-radius: 10px;
	padding: 10px;
	display: flex;
	align-items: center;
	font-weight: 600;
	font-size: 0.8em;
}
.comment-2-user image {
	height: 33px;
    width: 33px;
    margin-right: 8px;
    border-radius: 50%;
}
.comment-2-user > text {
	margin-left: auto;
	margin-right: 10px;
	display: flex;
	align-items: center;
}
.comment-2-text {
	padding: 0 10px;
	font-size: 0.9em;
}
.comment-2-time {
	padding: 10px;
	font-size: 0.8em;
	color: #999;
}
.comment-3 {
	background-color: #f8f8fa;
	padding: 10px;
	border-radius: 10px;
	width: 90%;
	margin: auto;
	font-size: 0.9em;
}
.comment-3-item {
	padding: 5px;
	overflow: hidden;
	word-break: break-all;
}
.comment-3-user {
	font-weight: 600;
	color: #999;
}

.commentAdd {
	position: fixed;
	left: 0;
	bottom: 0;
	padding: 10px 0 15px 0;
	width: 100%;
	background: #fff;
}

.commentAdd-input {
	margin: auto;
	padding: 10px;
	background: #f3f4f8;
	color: #1d1d1f;
	font-size: 15px;
	width: 88%;
	border-radius: 22px;
	display: flex;
	align-items: center;
}
.commentAdd-input > textarea {
	padding: 0 1em;
	max-height: 2.6em;
	flex-shrink: 1;
	display: flex;
	width: 100%;
	max-width: 100%;
}
